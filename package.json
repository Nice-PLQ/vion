{
  "name": "vion",
  "version": "1.0.0",
  "description": "A Vue.js 3.0 UI Components for Mobile Web",
  "main": "lib/index.js",
  "module": "es/index-es.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "npm run build:entry && cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:entry": "node script/build-entry.js",
    "build:site": "vite build",
    "prepare": "npm run build",
    "lint:js": "eslint src/**/*.{js,vue} --fix",
    "lint:css": "stylelint **/*.{css,less} --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "cross-env NODE_ENV=test jest"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "lib/",
    "es/",
    "types/",
    "vetur/",
    "notice.js",
    "README.md"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "lib/*",
    "es/*/style.js"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "author": "yangling",
  "license": "MIT",
  "browserslist": [
    "iOS >= 10.0",
    "Android >= 10.0"
  ],
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "autosize": "^5.0.1",
    "clipboard": "^2.0.6",
    "highlight.js": "^9.15.10",
    "qrcode.vue": "^3.3.3",
    "vion-svg-icon": "^1.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": ">=7.13.0 <8.0.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@types/jest": "^26.0.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.0.0-beta.13",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "esbuild": "^0.14.8",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^9.3.0",
    "extract-comments": "^1.1.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.1",
    "jest": "^25.4.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.13.1",
    "lint-staged": "^9.4.2",
    "postcss": "^8.4.5",
    "rollup": "^2.47.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "1.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "uppercamelcase": "^3.0.0",
    "vite": "^4.3.9",
    "vite-plugin-md": "^0.21.5",
    "vue-jest": "^5.0.0-alpha.10"
  }
}
